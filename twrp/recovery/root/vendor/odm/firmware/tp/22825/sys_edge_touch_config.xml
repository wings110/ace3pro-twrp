					<!-- 竖屏同一侧抓握消点方案，针对检测apk关闭 -->
					<command name="finger_hold_matched_ver_support"	value="mod para_V2 finger_hold_matched_ver_support:0"/>
				</activity>
			</packages>

		</device>
</filter-conf>
